\"rule\" required></v-textarea>\n                </v-row>\n              </v-form>\n            </v-card-text>\n            <v-divider></v-divider>\n            <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn color=\"primary\" text @click=\"dialog = false\"> Close </v-btn>\n              <v-btn color=\"primary\" text :disabled=\"!valid\" @click=\"submitData\"> Save </v-btn>\n            </v-card-actions>\n        </v-card>\n      </v-dialog>\n    </v-col>\n  </v-row>\n</v-container>\n</template>\n\n<script>\n\nexport default {\n  props:{\n    images: Boolean\n  },\n  data: function () {\n    return {\n      dialog: false,\n      image: {\n        caption: '',\n        file: [],\n        preview: ''\n      },\n      x: null,\n    // Validation\n      valid: true,\n      rule: [ v => !!v || 'Required' ],\n      imgRule: [ v => !v || v.size < 2000000 || 'Image size should be less than 2 MB!']\n    }\n  },\n  methods:{\n    async submitData(){\n      // this.$props.images.push(this.image)\n      await this.$emit('updateImages', this.image)\n      this.dialog = false\n      // this.$nextTick(() => {\n      //   this.image.caption = \"\"\n      // this.image.file = []\n      // this.image.preview = \"\"\n      // })\n      \n\n    },\n    change(e){\n      if (e)\n        this.image.preview = URL.createObjectURL(e)\n      else\n        this.image.preview = ''\n    },\n    reset() { this.$refs.form.reset() },\n    validate () { this.$refs.form.validate() },\n  },\n  watch:{\n    images: function(){\n        if (this.dialog){\n          this.image.caption = \"\"\n          this.image.file = []\n          this.image.preview = \"\"\n        }\n    }\n  }\n}\n</script>\n"]}]}