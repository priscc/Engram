ics\" item-text=\"title\" menu-props=\"auto\" label=\"Select a topic\" single-line ripple @change=\"topicData\"></v-select>\n\t\t\t</v-col>\n\t\t\t<v-spacer></v-spacer>\n\t\t</v-row>\n\t</v-container>\n\t<add-content :topicObj=\"topicObj\"></add-content>\n\n</div>\n</template>\n\n<script>\nimport addContent from '@/views/Adding_Content/AddContent.vue'\nimport { db } from '@/main'\nimport firebase from 'firebase'\nexport default {\n\tcomponents: {\n\t\taddContent\n\t},\n\tdata: function () { return {\n\t\t// Drop down\n\t\ttopics: [],\n\t\ttopic: '',\n\n\t\t// Toggle bar\n\t\ttab: 'Events',\n\t\ttabs: [\n\t\t\t{ title: 'Events', icon: 'mdi-clock-start', doc: 'Events' },\n\t\t\t{ title: 'Terminology', icon: 'mdi-view-dashboard', doc: 'Terminology' },\n\t\t\t{ title: 'Historical People', icon: 'mdi-account', doc: 'People' },\n\t\t\t{ title: 'Works of The Time', icon: 'mdi-book-open-variant', doc: 'Works' }\n\t\t],\n\t\t\n\t\t// sending to components\n\t\ttopicObj: {},\n\t}},\n\tmethods: {\n\n\t\t// grabs all topics from db\n\t\tasync listTopics () {\n\t\t\tawait db.collection('topics').get().then(doc => {\n\t\t\t\tdoc.forEach(doc => {\n\t\t\t\t\tvar topic = doc.data()\n\t\t\t\t\tvar id = doc.id\n\t\t\t\t\ttopic['id'] = id\n\t\t\t\t\tthis.topics.push(topic)\n\t\t\t\t})\n\t\t\t})\n\t\t},\n\n\t\t// isolates topic selected from drop down\n\t\ttopicData () {\n\t\t\tthis.topicObj = this.topics.find(t => t.title === this.topic);\n\t\t},\n\n\n\t},\n\tmounted () {\n\t\tthis.listTopics()\n\t}\n}\n</script>"]}]}