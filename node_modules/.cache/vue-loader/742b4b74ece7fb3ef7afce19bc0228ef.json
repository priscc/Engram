t\t <v-text-field   disabled prepend-icon=\"\" label=\"Subtopi Selected: American Revolution\" outlined shaped ></v-text-field>\n        \t</v-col>\n\t\t</v-row>\n\t\t<v-row class=\"pt-5\">\n\t\t\t<v-spacer></v-spacer>\n\t\t\t<v-col cols=\"4\">\n\t\t\t\t<!-- Type of Event -->\n\t\t\t\tSelect event type:<v-select disabled v-model=\"createdEvent.selectedEventType\" :items=\"typeOfEvent\"label=\"Event\" outlined >Cause</v-select>\n\t\t\t\t<!-- Event Title -->\n\t\t\t\t<v-text-field label=\"Enter event title:\" autocomplete=\"off\" v-model=\"createdEvent.eventTitle\"></v-text-field>\n\t\t\t\t\t<v-menu  ref=\"menu\" v-model=\"menu\" :close-on-content-click=\"false\" transition=\"scale-transition\" offset-y min-width=\"290px\">\n\t\t\t\t\t\t\n\t\t\t\t\t\t<template v-slot:activator=\"{ on }\" >\n\t\t\t\t\t\t\t<v-text-field v-model=\"createdEvent.date\" label=\"Date\" readonly v-on=\"on\" ></v-text-field>\n\t\t\t\t\t\t</template>\n\t\t\t\t\t\t<v-date-picker ref=\"picker\" v-model=\"createdEvent.date\" type=\"month\" :max=\"new Date().toISOString().substr(0, 10)\" min=\"1-01-01\" @change=\"save\" ></v-date-picker>\n\t\t\t\t\t</v-menu>\n\t\t\t</v-col>\n\t\t\t<!-- <v-spacer></v-spacer> -->\n\t\t\t<v-col cols=\"7\" class=\"ml-5 pl-5\" >\n\t\t\t\tContent: <v-textarea  height=\"200px\" outlined v-model=\"createdEvent.content\"  name=\"inputTextarea\" label=\"Description\" value=\"The Woodman set to work at once, and so sharp was his axe that the tree was soon chopped nearly through.\"></v-textarea> \n\t\t\t</v-col>\n\t\t</v-row>\t\n\t\t<!-- Submit Button -->\n\t\t<v-row class=\"pt-5\">\n\t\t\t<v-col class=\"d-flex align-end flex-column\">\n\t\t\t\t<v-btn large outlined color=\"success\" v-on:click.prevent=\"submitData\">SUBMIT</v-btn>\n\t\t\t</v-col>\n\t\t</v-row>\n\t\t\t\t\t\n\t</v-container>\n\t</div>\n</template>\n\n\n<script>\nimport  { db } from '@/main'\n\nexport default {\n\tdata: function(){\n\t\treturn{\n\t\t\tcreatedEvent: [{\n\t\t\t\teventType: \"Cause\",\n\t\t\t\teventTitle: \"\",\n\t\t\t\tdate: null,\n\t\t\t\tcontent: \"\"\n\t\t\t}],\n\t\t\ttypeOfInformation: [ 'Event', 'Terminology', 'Historical People'],\n\t\t\ttypeOfEvent: ['Cause', 'Turning Point', 'Effect'],\n\t\t\tmenu: false\n\t\t}\n\t},\n\twatch: {\n\t  menu (val) {\n\t\tval && setTimeout(() => (this.$refs.picker.activePicker = 'YEAR'))\n\t  }\n\t},\n\tmethods: {\n\t\tsave (date) {\n\t\t\tthis.$refs.menu.save(date)\n\t\t},\n\t\tasync submitData() {\n\t\t\tlet timeperiod =  this.timePeriods.indexOf(this.selectedTimePeriod) + 1;\n\t\t\tlet todosRef = await db.collection('time_periods');\n\t\t\tlet id = todosRef.doc(\"time_period_\" + timeperiod);\n\t\t\t//getting time period currently existing subtopics\n\t\t\tvar subtopicsAdded = await id.get().then(function(doc){\n\t\t\t\treturn doc.data().subtopics;\n\t\t\t});\n\t\t\t//creating URL & special collection name for this subtopic\n\t\t\tvar url = this.createdSubtopic.subtopicTitle.replace(/\\s+/g, '').toLowerCase();\n\t\t\tthis.createdSubtopic.subtopic_url = url;\n\t\t\t//adding subtopic info to the time period\n\t\t\tsubtopicsAdded.push(this.createdSubtopic);\n\t\t\t//UPDATING time period DOCUMENT\n\t\t\tid.update({\n\t\t\t\tsubtopics: subtopicsAdded\n\t\t\t});\n\t\t\t//CREATING a new subtopic COLLECTION\n\t\t\t// if(this.createdSubtopic.subtopic_published){\n\t\t\t// \tconsole.log(\"true\")\n\t\t\t// \tdb.doc(url + \"/\" + this.informationType).set({\n\t\t\t// \t\tArrayOfevents: this.form\n\t\t\t// \t});\n\t\t\t// }\n\t\t}\n\t}\n}\n</script>\n\n\n\n\t\t"]}]}