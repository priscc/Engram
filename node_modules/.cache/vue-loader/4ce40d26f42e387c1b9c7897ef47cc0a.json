end font-weight-black\">{{event.title}}</h1>\n\t\t\t<p v-if=\"event.endDate\" class=\"pt-5 d-flex align-end font-italic mb-2\"> ({{event.startDate}} - {{event.endDate}})</p>\n\t\t\t<p v-else class=\"pt-5 d-flex align-end font-italic mb-2\"> ({{event.startDate}})</p>\n\t\t</v-col>\n\t</v-row>\n\n\t<!-- Content -->\n\t<v-row>\n\t\t<v-spacer></v-spacer>\t\t\n\t\t<v-col lg=\"11\" md=\"11\" cols=\"12\" class=\"pl-0\">\n\t\t\t<v-row class=\"pl-0\">\n\t\t\t\t<v-col lg=\"6\" md=\"6\" cols=\"12\" class=\"pl-0\">\n\t\t\t\t\t<div class=\"pt-5 pr-12\" :key=\"event.contentMD\" v-markdown>{{event.contentMD}}</div>\n\t\t\t\t</v-col>\n\n\t\t\t\t<!-- Resources -->\n\t\t\t\t<v-col class=\"pl-0\">\n\t\t\t\t\t<resources :images=\"images\" :articles=\"articles\" :videos=\"videos\" ></resources>\n\t\t\t\t</v-col>\n\t\t\t</v-row>\t\t\t\n\t\t</v-col>\n\t</v-row>\n</v-container>\n</template>\n\n\n\n<script>\nimport { db, analytics } from '@/main'\nimport resources from '@/components/Resources.vue'\n\nexport default {\n\tcomponents: {\n\t\tresources\n\t},\n\tdata () { return {\n\t\tid: this.$route.params.id,\n\t\tevent: {},\n\t\toverlay: false,\n\t\tresources: [],\n\t\timages: [],\n\t\tarticles: [],\n\t\tvideos: []\n\t}},\n\tmethods: {\n\n\t\tasync pullData () {\n\t\t\tawait db.collection('events').doc(this.id).get().then(function(doc) {\n\t\t\t\tthis.event = doc.data()\n\t\t\t}.bind(this))\n\n\n\t\t\tawait db.collection('resources').where('parentID', '==', this.id).get().then(function(querySnapshot) {\n\t\t\t\tconsole.log('here')\n\t\t\t\t\tquerySnapshot.forEach(function(doc) {\n\t\t\t\t\t\tthis.resources.push(doc.data())\n\t\t\t\t\t}.bind(this));\n\t\t\t\t}.bind(this))\n\n\t\t\tthis.resourcesSort()\n\t\t},\n\n\t\tresourcesSort (){\n\t\t\tanalytics.logEvent('Event_Page', { event: this.event.title } );\n\n\t\t\tvar rArr = this.resources\n\n\t\t\trArr.forEach( r => {\n\t\t\t\tif( r.resourceType == 'image') {\n\t\t\t\t\tthis.images.push(r)\n\t\t\t\t}\n\t\t\t\telse if( r.resourceType == 'article') {\n\t\t\t\t\tthis.articles.push(r)\n\t\t\t\t}\n\t\t\t\telse{\n\t\t\t\t\tthis.videos.push(r)\n\t\t\t\t}\n\t\t\t})\n\t\t},\n\t\t// back: goes to previous page\n\t\tback () {\n\t\t\tthis.$router.go(-1)\n\t\t},\n\n\t\t\n\t},\n\tmounted () {\n\t\twindow.scrollTo({ top: 0, left: 0, behavior: 'smooth' });\n\t\tthis.pullData()\n\t}\n}\n</script>\n\n\n\n\n<style scoped>\n\t#text{\n\t\tposition: relative;\n\t\tcolor: white;\n\t\tmax-height: 90%;\n\t}\n\t#button{\n\t\tposition: absolute;\n\t\ttop: 10%;\n\t\tright: 5%;\n\t\tcolor: white;\n\t\ttransform: translate(-50%,-50%);\n\t\t-ms-transform: translate(-50%,-50%);\n\t}\n\t.profile{\n\t\tborder: 1px solid black;\n\t\tborder-radius: 50%;\n\t}\n</style>\n"]}]}