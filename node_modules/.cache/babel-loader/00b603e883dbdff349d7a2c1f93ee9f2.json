\t</v-row>\n\n</v-container>\n</template>\n\n<script>\n\nexport default {\n  name: 'Topics',\n  props: {\n    events: Array,\n    header: String\n  },\n  methods: {\n    hover (d, i) {\n      var num = i.label\n      var circle = document.getElementById('circle' + num)\n      var tool = document.getElementsByClassName('tooltip')[0]\n      \n      if (d) {\n        circle.style.transitionDelay = 'all 2s'\n        circle.style.r = 15\n        circle.style.stroke = 'black'\n        circle.style.strokeWidth = '1.25'\n        \n        var pageX = circle.getBoundingClientRect().left\n        var pageY = circle.getBoundingClientRect().top\n       \n        d3.select(tool)\n\t      .transition()\n\t      .duration(200)\n\t      .style('opacity', 0.9)\n\t      .text(i.name + \" (\" + i.date + \")\")\n\t      .style('left', pageX + 'px')\n\t      .style('top', (pageY - 100)+ 'px')\n      } else {\n        circle.style.r = 20 / 1.8\n        circle.style.stroke = 'none'\n        \n        d3.select(tool).transition()\n\t      .duration(50)\n\t      .style('opacity', 0)\n      }\n    }\n  }\n\n}\n</script>\n\n<style type=\"text/css\" scoped>\n\t.outlineItem{\n\t\t/*border: 1px solid lightgrey;*/\n\t\t/*opacity: .5*/\n\t}\n\t.header{\n\t\tborder: 1px solid black;\n\t}\n\t.scrollEvents{\n\t\theight: 320px;\n\t\toverflow: scroll;\n\t}\n\t.badgeText{\n\t\tcolor: white;\n\t\twidth: 100%;\n\t\tbackground-color: #FF0000;\n\t\tborder-radius: 25px;\n\n\t}\n\n</style>\n"],"sourceRoot":"src/components/Events"}]}