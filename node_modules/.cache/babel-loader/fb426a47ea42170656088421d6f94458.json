KAlCA;AAqCA;AACA,IAAA,WAtCA,uBAsCA,IAtCA,EAsCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,SADA,GACA,QAAA,CAAA,OAAA,GAAA,GAAA,GAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CADA;AAAA;AAAA,uBAEA,SAAA,CAAA,MAAA,GACA,KADA,CACA,UAAA,KAAA,EAAA;AAAA,kBAAA,OAAA,CAAA,KAAA,CAAA,2BAAA,EAAA,KAAA;AAAA,iBADA,CAFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KA1CA;AA6CA;AACA,IAAA,SA9CA,qBA8CA,IA9CA,EA8CA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,UADA,GACA,QAAA,CAAA,OAAA,GAAA,GAAA,GAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,IAAA,CADA;AAAA;AAAA,uBAEA,UAAA,CAAA,GAAA,CAAA,IAAA,CAAA,SAAA,CAFA;;AAAA;AAGA,gBAAA,IAAA,CAAA,SAAA,GAAA,IAAA,CAAA,SAAA,CAAA,IAAA;AAHA;AAAA,uBAIA,UAAA,CAAA,cAAA,EAJA;;AAAA;AAIA,gBAAA,IAAA,CAAA,QAJA;AAKA,uBAAA,IAAA,CAAA,SAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,WAAA,EAAA,IAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KArDA;AAuDA,IAAA,KAvDA,mBAuDA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,KA5DA;AA6DA,IAAA,QA7DA,sBA6DA;AAAA,WAAA,KAAA,CAAA,IAAA,CAAA,QAAA;AAAA;AA7DA;AApBA,CAAA","sourcesContent":["<template>\n<div id=\"app\">\n\n\t<!-- Adding new Work  -->\n\t<v-container fluid class=\"pl-9\">\n\t\t<v-form ref=\"form\" v-model=\"valid\">\n\t\t\t<v-row>\n\t\t\t\t<v-col cols=\"2\">\n\t\t\t\t\t<v-file-input dense v-model=\"newWork.thumbnail\" accept=\"image/*\" counter show-size label=\"Thumbnail\" placeholder=\"Thumbnail\" :rules=\"imgRuleNew\" clearable></v-file-input>\n\t\t\t\t</v-col>\n\t\t\t\t<v-col cols=\"4\">\n\t\t\t\t\t<v-text-field \n\t\t\t\t\t\tv-model=\"newWork.title\" label=\"Title\" dense :rules=\"rule\" required>\n\t\t\t\t\t</v-text-field>\n\t\t\t\t\t<v-textarea outlined rows=\"2\" v-model=\"newWork.caption\" label=\"Caption\" :rules=\"rule\" required></v-textarea>\n\t\t\t\t</v-col>\n\t\t\t\t<v-col class=\"d-flex flex-row justify-center pt-8\" cols=\"2\">\n\t\t\t\t\t<v-btn fab class=\"white--text\" :disabled=\"!valid\" color=\"green\" @click=\"add\"><v-icon>mdi-plus</v-icon></v-btn>\n\t\t\t\t\t<v-btn outlined class=\"white--text ml-3 mt-3\" color=\"red\" @click=\"reset\">RESET</v-btn>\n\t\t\t\t</v-col>\n\t\t\t\t<v-spacer></v-spacer>\n\t\t\t</v-row>\n\t\t</v-form>\n\t</v-container>\n\t\n\t<!-- Display works -->\n\t<v-container>\n\t\t<v-row class=\"pt-5\">\n\t\t\t<v-col v-for=\"(w, index) in works\" :key=\"index\" cols=\"4\">\n\t\t\t\t<v-card class=\"pa-2 d-flex flex-column\">\n\t\t\t\t\t<v-img height=\"125\" :src=\"w.thumbURL\"></v-img>\n\t\t\t\t\t<v-card-text>{{w.title}}</v-card-text>\n\t\t\t\t\t<v-card-subtitle>{{w.caption}}</v-card-subtitle>\n\t\t\t\t\t<v-btn fab x-small absolute top right @click=\"[dialog = true, work = w]\">\n\t\t\t\t\t\t<v-icon>mdi-lead-pencil</v-icon>\n\t\t\t\t\t</v-btn>\n\t\t\t\t</v-card>\n\t\t\t</v-col>\n\t\t</v-row>\n\t</v-container>\n\n\t<!-- Revise work -->\n\t<v-dialog v-model=\"dialog\" width=\"500px\">\n\t\t<v-card elevation=\"4\">\n\t\t\t<v-btn color=\"orange lighten-1\" small fab absolute right dark rounded @click=\"reset()\"><v-icon dark>mdi-close</v-icon></v-btn>\n\t\t\t<v-form class=\" pt-6\" ref=\"form\" v-model=\"valid\">\n\t\t\t\t<v-container>\n\t\t\t\t\t<v-row>\n\t\t\t\t\t\t<v-col>\n\t\t\t\t\t\t\t<v-file-input dense v-model=\"work.thumbnail\" accept=\"image/*\" counter show-size label=\"Thumbnail\" :placeholder=\"work.thumbFile\" :rules=\"imgRuleMod\" clearable></v-file-input>\n\t\t\t\t\t\t\t<v-text-field \n\t\t\t\t\t\t\t\tv-model=\"work.title\" label=\"Term\" :placeholder=\"work.title\" dense >\n\t\t\t\t\t\t\t</v-text-field>\n\t\t\t\t\t\t\t<v-textarea outlined rows=\"2\" v-model=\"work.caption\" :placeholder=\"work.caption\"  label=\"Definition\"></v-textarea>\n\t\t\t\t\t\t</v-col>\n\t\t\t\t\t</v-row>\n\t\t\t\t</v-container>\n\t\t\t</v-form>\n\t\t\t<v-card-actions class=\"d-flex justify-end\">\n\t\t\t\t<v-btn class=\"white--text\" color=\"green\" @click=\"[submit(), dialog = false]\">Save</v-btn>\n\t\t\t</v-card-actions>\n\t\t</v-card>\n\t</v-dialog>\n</div>\n</template>\n\n\n<script type=\"text/javascript\">\nimport { db } from '@/main'\nimport store from \"@/store\";\nimport _ from 'lodash'\nimport firebase from 'firebase'\nexport default {\n\tdata: function () {return {\n\t\tdialog: false,\n\t\tnewWork: {}, //{thumbnail:'', title:'', caption:''}\n\t\twork: {}, //form creates objects -- {thumbnail:'', title:'', caption:''} (thumnail is removed)\n\t\t// Validation\n\t\tvalid: true,\n\t\trule: [ v => !!v || 'Required' ],\n\t\timgRuleNew: [ \n\t\t\tv => !v || v.size > 0 && v.size < 2000000 || 'Image larger than 2MB',\n\t\t],\n\t\timgRuleMod: [ v => !v || v.size > 0 && v.size < 2000000 || 'Image required' ]\n\t}},\n\n\tcomputed: {\n\t\tworks() {\n\t\t\treturn this.$store.state.works\n\t\t}\n\t},\n\n\tmethods: {\n\n\t\tasync add(){\n\t\t\tvar keywords = _.words(this.newWork.title);\n\n\t\t\tthis.newWork.topicID = this.$store.state.topic.id\n\t\t\tthis.newWork.searchArray = keywords\n\t\t\t\t\n\t\t\tif(this.newWork.thumbnail){\n\t\t\t\tawait this.saveImage(this.newWork)\n\t\t\t}\n\t\t\t\n\t\t\tdb.collection(\"works\").add(this.newWork)\n\t\t\t.then(function() { console.log(\"Document successfully updated!\"); })\n\t\t\t.catch(function(error) { console.error(\"Error updating document:\", error); });\n\t\t\t\n\t\t\t\tconsole.log(\"addWork\", this.newWork)\n\t\t\tstore.dispatch(\"addWork\", this.newWork);\n\n\t\t\tthis.reset()\n\t\t},\n\n\t\tasync submit(){\n\t\t\tconsole.log('in here')\n\t\t\tif(this.work.thumbnail){\n\t\t\t\tawait this.deleteImage(this.work)\n\t\t\t\tawait this.saveImage(this.work)\n\t\t\t}\n\t\t\tconsole.log(this.work.id)\n\t\t\tdb.collection(\"works\").doc(this.work.id).update(this.work)\n\t\t\t\t.then(function() { console.log(\"Document successfully updated!\"); })\n\t\t\t\t.catch(function(error) { console.error(\"Error updating document:\", error); });\n\n\t\t\tthis.reset()\n\t\t},\n\n\n\t\t//clears firebase storage - thumbFile - thumbURL\n\t\tasync deleteImage(file){\n\t\t\tvar desertRef = firebase.storage().ref().child(file.thumbFile);\n\t\t\tawait desertRef.delete()\n\t\t\t.catch(function(error) { console.error(\"Error updating document: \", error) });\n\t\t},\n\n\n\t\t//updates firebase storeage - thumbFile - thumbURL - deletes thumbURL\n\t\tasync saveImage(file){\n\t\t\tvar storageRef = firebase.storage().ref().child(file.thumbnail.name)\n\t\t\tawait storageRef.put(file.thumbnail)\n\t\t\tfile.thumbFile = file.thumbnail.name\n\t\t\tfile.thumbURL = await storageRef.getDownloadURL()\n\t\t\tdelete file.thumbnail\n\t\t\tconsole.log(\"img added\", file)\n\t\t},\n\n\t\treset () { \n\t\t\tthis.work = {}\n\t\t\tthis.newWork = {}\n\t\t\tthis.dialog = false\n\t\t\tthis.$refs.form.reset() \n\t\t},\n\t\tvalidate () { this.$refs.form.validate() },\n\t}\n}\n</script>"],"sourceRoot":"src/components/Content_Forms/Modifying_Forms"}]}