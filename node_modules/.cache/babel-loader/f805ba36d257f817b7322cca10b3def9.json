 Topic Components -->\n\t<v-container fluid>\n\t\t<v-row v-if=\"topic != '' \">\n\t\t\t<v-col>\n\t\t\t\t<!-- Tab bar -->\n\t\t\t\t\t<v-card outlined>\n\t\t\t\t\t\t<v-tabs v-model=\"tab\" centered icons-and-text color=\"pink\">\n\t\t\t\t\t\t\t<v-tabs-slider></v-tabs-slider>\n\t\t\t\t\t\t\t<v-tab class=\"py-2\" v-for=\"(t, i) in tabs\" :key=\"i\" :href=\"`#`+t.doc\">\n\t\t\t\t\t\t\t\t<p class=\"caption\">{{t.title}}</p>\n\t\t\t\t\t\t\t\t<v-icon medium>{{t.icon}}</v-icon>\n\t\t\t\t\t\t\t</v-tab>\n\t\t\t\t\t\t</v-tabs>\n\t\t\t\t\t</v-card>\n\t\t\t\t\t<!-- Different Content Forms -->\n\t\t\t\t\t<v-tabs-items v-model=\"tab\">\n\t\t\t\t\t\t<v-tab-item v-for=\"(t, i) in tabs\" :key=\"i\" :value=\"t.doc\" >\n\t\t\t\t\t\t\t<mod-events v-if=\"tab == 'Events'\" :topicObj=\"topicObj\"></mod-events>\n\t\t\t\t\t\t\t<!-- <add-term v-if=\"tab == 'Terminology'\" :topicObj=\"topicObj\" @updateTopic=\"updateTopic\"></add-term> -->\n\t\t\t\t\t\t\t<!-- <add-people v-if=\"tab == 'People'\" :topicObj=\"topicObj\" @updateTopic=\"updateTopic\"></add-people> -->\n\t\t\t\t\t\t\t<!-- <add-works v-if=\"tab == 'Works'\" :topicObj=\"topicObj\" @updateTopic=\"updateTopic\"></add-works> -->\n\t\t\t\t\t\t</v-tab-item>\n\t\t\t\t\t</v-tabs-items>\n\t\t\t</v-col>\n\t\t</v-row>\n\t</v-container>\n</div>\n</template>\n\n<script>\nimport modEvents from '@/components/Content_Forms/Modifying_Forms/ModifyingEvents.vue'\n// import modTerm from '@/components/Content_Forms/Add_Forms/TerminologyForm.vue'\n// import modPeople from '@/components/Content_Forms/Add_Forms/PeopleForm.vue'\n// import modWorks from '@/components/Content_Forms/Add_Forms/WorksForm.vue'\nimport { db } from '@/main'\nimport firebase from 'firebase'\nimport store from \"@/store\"\n\n\nexport default {\n\tcomponents: {\n\t\tmodEvents,\n\t\t// modTerm,\n\t\t// modPeople,\n\t\t// modWorks\n\t},\n\tdata: function () { return {\n\t\t// Drop down\n\t\ttopics: [],\n\t\ttopic: '',\n\n\t\t// Toggle bar\n\t\ttab: 'Events',\n\t\ttabs: [\n\t\t\t{ title: 'Events', icon: 'mdi-clock-start', doc: 'Events' },\n\t\t\t{ title: 'Terminology', icon: 'mdi-view-dashboard', doc: 'Terminology' },\n\t\t\t{ title: 'Historical People', icon: 'mdi-account', doc: 'People' },\n\t\t\t{ title: 'Works of The Time', icon: 'mdi-book-open-variant', doc: 'Works' }\n\t\t],\n\t\t\n\t\t// sending to components\n\t\ttopicObj: {},\n\t\tevents: []\n\t}},\n\tfirestore() {\n\t\treturn {\n\t\t\tevents: db.collection(\"events\"),\n\t\t\t// cards: db.collection(\"cards\")\n\t\t};\n\t},\n\tmethods: {\n\n\t\t// grabs all topics from db\n\t\tasync listTopics () {\n\t\t\tawait db.collection('topics').get().then(doc => {\n\t\t\t\tdoc.forEach(doc => {\n\t\t\t\t\tvar topic = doc.data()\n\t\t\t\t\tvar id = doc.id\n\t\t\t\t\ttopic['id'] = id\n\t\t\t\t\tthis.topics.push(topic)\n\t\t\t\t})\n\t\t\t})\n\t\t},\n\n\t\t// isolates topic selected from drop down\n\t\ttopicData () {\n\t\t\tvar topicObj = this.topics.find(t => t.title === this.topic);\n\t\t\tstore.dispatch(\"modTopic\", topicObj);\n\t\t\tconsole.log('mod', store.state.modTopic)\n\t\t\tconsole.log('events', this.events)\n\t\t\t// this.topicObj = this.topics.find(t => t.title === this.topic);\n\t\t},\n\n\t\t\n\n\t},\n\tmounted () {\n\t\tthis.listTopics()\n\t}\n}\n</script>"],"sourceRoot":"src/views/Adding_Content"}]}